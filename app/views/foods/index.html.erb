<h2>Results</h2>

<%n = 1%>
<%@results.each do |food|%>

<div id="food_result">
<h4>Results Number <%=n%></h4>
  
  <div id="food_brand">
    <p><%=food.brand%></p>
  </div>

  <div id="food_description">
    <p><%=food.description%></p>
  </div>

  
  <p><%=food.gtin%></p>
  <p><%=food.ingredients%></p>
  <br>
  <br>
  <br>
</div>
<%n = n+1%>
<% end %>
